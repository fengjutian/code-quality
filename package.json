{
  "name": "code-quality",
  "displayName": "VSCode Code Quality",
  "description": "VSCode plugin to analyze code quality",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:extension.analyzeCode",
    "onSaveTextDocument"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "echo \"Postinstall completed\""
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "@types/vscode": "^1.80.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.analyzeCode",
        "title": "Analyze Code Quality"
      }
    ]
  }
}
